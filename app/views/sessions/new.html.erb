<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  


  <%= f.submit "Log in" %>
<% end %>

<br>
<%= link_to "ブログ一覧画面へ！", blogs_path, class: "btn btn-primary btn-md" %>

