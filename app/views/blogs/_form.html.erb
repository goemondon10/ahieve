<%= form_with(model: @blog, local: true ,url: choose_new_or_edit) do |form| %>

  
<p><%= notice %></p>

<div class="blogs_title">
  <%= form.label :title %>
  <%= form.text_field :title %>
</div>

<div class="blogs_title">
  <%= form.label :content %>
  <%= form.text_field :content %>
</div>  

<div class="field">
  <%= form.label :image %>
  <%= form.file_field :image, id: :blog_image %>
</div>


<%= form.submit %>
<% end %>

<%= link_to "blogs一覧画面にもどる", blogs_path %>

 